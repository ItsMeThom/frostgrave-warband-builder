; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="FrostgraveWarbandBuilder"
run/main_scene="res://scenes/ui_main.tscn"
config/features=PackedStringArray("4.1", "Mobile")
boot_splash/bg_color=Color(0.141176, 0.141176, 0.141176, 1)
boot_splash/image="res://Resources/images/wiz_boot.png"
boot_splash/use_filter=false
config/icon="res://icon.svg"

[autoload]

Data="*res://scenes/data.tscn"
UsrData="*res://scenes/user_data.tscn"
UiColorSchemeResource="*res://Resources/ColourSchemes/UIColorSchemeResource.gd"

[display]

window/size/viewport_width=390
window/size/viewport_height=840
window/subwindows/embed_subwindows=false
window/stretch/mode="canvas_items"
window/handheld/orientation=1

[gui]

theme/default_font_multichannel_signed_distance_field=true

[input_devices]

pointing/emulate_touch_from_mouse=true

[rendering]

renderer/rendering_method="mobile"
textures/vram_compression/import_etc2_astc=true
